{
	"name": "@starbunk/shared",
	"version": "1.10.1",
	"description": "Shared services and utilities for Starbunk containers",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": "./dist/index.js",
		"./*": "./dist/*.js",
		"./observability/mixins": "./dist/observability/mixins/index.js",
		"./observability/*": "./dist/observability/*.js",
		"./services/llm": "./dist/services/llm/index.js",
		"./services/llm/*": "./dist/services/llm/*.js",
		"./services/vector": "./dist/services/vector/index.js",
		"./services/vector/*": "./dist/services/vector/*.js"
	},
	"files": [
		"dist/**/*",
		"package.json",
		"README.md"
	],
	"scripts": {
		"build": "tsc --build && tsc-alias -p tsconfig.json",
		"dev": "tsc --build --watch",
		"test": "vitest run",
		"test:watch": "vitest",
		"type-check": "tsc --build --dry",
		"clean": "tsc --build --clean",
		"lint": "eslint --ext .ts src",
		"lint:fix": "eslint --ext .ts src --fix"
	},
	"dependencies": {
		"@google/generative-ai": "^0.24.1",
		"@loglayer/transport-pino": "^2.2.17",
		"chalk": "^4.1.2",
		"date-fns": "^4.1.0",
		"discord.js": "^14.18.0",
		"dotenv": "^16.5.0",
		"envalid": "^8.0.0",
		"ioredis": "^5.3.2",
		"loglayer": "^8.4.0",
		"node-fetch": "^2.7.0",
		"node-schedule": "^2.1.1",
		"openai": "^5.3.0",
		"pino": "^10.2.0",
		"prom-client": "^15.1.3",
		"rxjs": "^7.8.2",
		"uuid": "^11.1.0",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@types/ioredis": "^5.0.0",
		"@types/node": "^20.19.27",
		"@types/node-fetch": "^2.6.13",
		"@types/node-schedule": "^2.1.7",
		"@types/uuid": "^10.0.0",
		"@vitest/ui": "^4.0.17",
		"rimraf": "^5.0.1",
		"tsc-alias": "^1.8.16",
		"typescript": "~5.3.0",
		"vitest": "^4.0.17"
	},
	"peerDependencies": {
		"typescript": "~5.3.0"
	}
}
