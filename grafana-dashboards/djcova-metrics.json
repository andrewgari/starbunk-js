{
  "dashboard": {
    "id": null,
    "uid": "starbunk-djcova",
    "title": "Starbunk Discord Bot - DJCova Music Service",
    "tags": ["starbunk", "discord", "djcova", "music", "voice"],
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 37,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "name": "guild_id",
          "type": "query",
          "query": "label_values(discord_messages_processed_total{service=\"djcova\"}, guild_id)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Music Commands (Last Hour)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(discord_messages_processed_total{service=\"djcova\", guild_id=~\"$guild_id\"}[1h]))",
            "legendFormat": "Total Commands"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Active Voice Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(voice_connections_active{service=\"djcova\", guild_id=~\"$guild_id\"})",
            "legendFormat": "Active Connections"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 5
                },
                {
                  "color": "red",
                  "value": 10
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Songs Queued (Last Hour)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(music_songs_queued_total{service=\"djcova\", guild_id=~\"$guild_id\"}[1h]))",
            "legendFormat": "Songs Queued"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Songs Played (Last Hour)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(music_songs_played_total{service=\"djcova\", guild_id=~\"$guild_id\"}[1h]))",
            "legendFormat": "Songs Played"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Music Command Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum by (command) (rate(music_commands_total{service=\"djcova\", guild_id=~\"$guild_id\"}[5m]))",
            "legendFormat": "{{command}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "normal",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "axisGridShow": true,
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 6
        }
      },
      {
        "id": 6,
        "title": "Voice Connection Duration",
        "type": "timeseries",
        "targets": [
          {
            "expr": "voice_connection_duration_seconds{service=\"djcova\", guild_id=~\"$guild_id\"}",
            "legendFormat": "Guild {{guild_id}} - Channel {{channel_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "axisGridShow": true,
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 6
        }
      },
      {
        "id": 7,
        "title": "Queue Status",
        "type": "timeseries",
        "targets": [
          {
            "expr": "music_queue_length{service=\"djcova\", guild_id=~\"$guild_id\"}",
            "legendFormat": "Guild {{guild_id}} - Queue Length"
          },
          {
            "expr": "music_queue_duration_seconds{service=\"djcova\", guild_id=~\"$guild_id\"} / 60",
            "legendFormat": "Guild {{guild_id}} - Queue Duration (min)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "axisGridShow": true,
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byRegexp",
                "options": ".*Queue Duration.*"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "m"
                }
              ]
            }
          ]
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 14
        }
      },
      {
        "id": 8,
        "title": "Audio Processing Errors",
        "type": "timeseries",
        "targets": [
          {
            "expr": "increase(music_errors_total{service=\"djcova\", guild_id=~\"$guild_id\"}[5m])",
            "legendFormat": "{{error_type}} - Guild {{guild_id}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "bars",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 100,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "normal",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "axisGridShow": true,
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "color": {
              "mode": "palette-classic"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 14
        }
      },
      {
        "id": 9,
        "title": "Most Popular Music Sources",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (source) (increase(music_songs_played_total{service=\"djcova\", guild_id=~\"$guild_id\"}[1h]))",
            "legendFormat": "{{source}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "displayMode": "table",
              "calculation": "lastNotNull"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 22
        }
      },
      {
        "id": 10,
        "title": "Voice Channel Activity",
        "type": "table",
        "targets": [
          {
            "expr": "sum by (guild_id, channel_name) (voice_connections_active{service=\"djcova\", guild_id=~\"$guild_id\"})",
            "legendFormat": "",
            "format": "table",
            "instant": true,
            "refId": "A"
          },
          {
            "expr": "sum by (guild_id, channel_name) (voice_connection_duration_seconds{service=\"djcova\", guild_id=~\"$guild_id\"})",
            "legendFormat": "",
            "format": "table",
            "instant": true,
            "refId": "B"
          }
        ],
        "transformations": [
          {
            "id": "seriesToColumns",
            "options": {
              "byField": "channel_name"
            }
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "Time": true,
                "job": true,
                "instance": true,
                "service": true
              },
              "indexByName": {
                "guild_id": 0,
                "channel_name": 1,
                "Value #A": 2,
                "Value #B": 3
              },
              "renameByName": {
                "guild_id": "Guild ID",
                "channel_name": "Voice Channel",
                "Value #A": "Active",
                "Value #B": "Duration (s)"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto",
              "inspect": false
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "Inactive"
                  },
                  "1": {
                    "text": "Active"
                  }
                },
                "type": "value"
              }
            ]
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Duration (s)"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "s"
                }
              ]
            }
          ]
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 22
        }
      },
      {
        "id": 11,
        "title": "Command Response Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(music_command_duration_ms_bucket{service=\"djcova\", guild_id=~\"$guild_id\"}[5m])) by (le, command))",
            "legendFormat": "{{command}} - 50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(music_command_duration_ms_bucket{service=\"djcova\", guild_id=~\"$guild_id\"}[5m])) by (le, command))",
            "legendFormat": "{{command}} - 95th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "axisGridShow": true,
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 2000
                },
                {
                  "color": "red",
                  "value": 5000
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 30
        }
      }
    ]
  }
}