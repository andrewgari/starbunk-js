{
	"name": "@starbunk/djcova",
	"version": "1.0.0",
	"description": "DJCova - Music service container",
	"main": "dist/index-minimal.js",
	"scripts": {
		"build": "tsc && tsc-alias",
		"start": "node dist/index-minimal.js",
		"dev": "cross-env NODE_ENV=development DEBUG=true ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index-minimal.ts",
		"test": "jest",
		"type-check": "tsc --noEmit",
		"clean": "rimraf dist"
	},
	"dependencies": {
		"@starbunk/shared": "file:../shared",
		"@discordjs/builders": "^1.6.5",
		"@discordjs/rest": "^2.5.0",
		"@discordjs/voice": "^0.18.0",
		"@distube/yt-dlp": "^2.0.1",
		"@distube/ytdl-core": "^4.16.10",
		"discord.js": "^14.18.0",
		"dotenv": "^16.5.0",
		"ffmpeg-static": "^5.2.0",
		"opusscript": "^0.0.8",
		"play-dl": "^1.9.7",
		"ytdl-core": "^4.11.5",
		"chalk": "^4.1.2"
	},
	"devDependencies": {
		"@types/node": "^20.17.14",
		"@types/jest": "^29.5.14",
		"typescript": "~5.3.0",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsc-alias": "^1.8.11",
		"tsconfig-paths": "^4.2.0",
		"jest": "^29.7.0",
		"ts-jest": "^29.2.6",
		"cross-env": "^7.0.3",
		"rimraf": "^5.0.1"
	}
}
