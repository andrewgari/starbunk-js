{
  "dashboard": {
    "id": null,
    "uid": "starbunk-system-overview",
    "title": "Starbunk Discord Bot - System Overview",
    "tags": ["starbunk", "discord", "overview", "system"],
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 37,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "name": "service",
          "type": "query",
          "query": "label_values(discord_messages_processed_total, service)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        },
        {
          "name": "guild_id",
          "type": "query",
          "query": "label_values(discord_messages_processed_total{service=~\"$service\"}, guild_id)",
          "refresh": 2,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "System Status Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "count by (service) (up{job=~\"starbunk.*\"})",
            "legendFormat": "{{service}} - Online"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Total Messages Processed (Last Hour)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(discord_messages_processed_total{service=~\"$service\", guild_id=~\"$guild_id\"}[1h]))",
            "legendFormat": "Messages"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 8,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Bot Responses (Last Hour)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(bot_responses_total{service=~\"$service\", guild_id=~\"$guild_id\"}[1h]))",
            "legendFormat": "Responses"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 16,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Message Processing Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum by (service) (rate(discord_messages_processed_total{service=~\"$service\", guild_id=~\"$guild_id\"}[5m]))",
            "legendFormat": "{{service}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "axisGridShow": true,
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 6
        }
      },
      {
        "id": 5,
        "title": "Bot Response Rate by Service",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum by (service) (rate(bot_responses_total{service=~\"$service\", guild_id=~\"$guild_id\"}[5m]))",
            "legendFormat": "{{service}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "axisGridShow": true,
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 6
        }
      },
      {
        "id": 6,
        "title": "Memory Usage by Service",
        "type": "timeseries",
        "targets": [
          {
            "expr": "memory_usage_bytes{service=~\"$service\", type=\"heap_used\"} / 1024 / 1024",
            "legendFormat": "{{service}} - Heap Used"
          },
          {
            "expr": "memory_usage_bytes{service=~\"$service\", type=\"rss\"} / 1024 / 1024",
            "legendFormat": "{{service}} - RSS"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "decmbytes",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "axisGridShow": true,
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 500
                },
                {
                  "color": "red",
                  "value": 1000
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 14
        }
      },
      {
        "id": 7,
        "title": "Circuit Breaker Status",
        "type": "timeseries",
        "targets": [
          {
            "expr": "circuit_breaker_open{service=~\"$service\"}",
            "legendFormat": "{{service}} - {{bot}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "stepBefore",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 20,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "axisGridShow": true,
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "area"
              }
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "Closed"
                  },
                  "1": {
                    "text": "Open"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 14
        }
      },
      {
        "id": 8,
        "title": "Top Active Channels",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, channel_messages_per_minute{service=~\"$service\", guild_id=~\"$guild_id\"})",
            "legendFormat": "",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "Time": true,
                "job": true,
                "instance": true
              },
              "indexByName": {
                "channel_name": 0,
                "guild_id": 1,
                "service": 2,
                "Value": 3
              },
              "renameByName": {
                "channel_name": "Channel",
                "guild_id": "Guild ID",
                "service": "Service",
                "Value": "Messages/Min"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto",
              "inspect": false
            },
            "mappings": []
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Messages/Min"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "short"
                }
              ]
            }
          ]
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 22
        }
      }
    ]
  }
}