{
	"name": "@starbunk/covabot",
	"version": "1.0.0",
	"description": "CovaBot - AI personality bot container",
	"main": "dist/index-minimal.js",
	"scripts": {
		"build": "tsc && tsc-alias",
		"start": "node dist/index-minimal.js",
