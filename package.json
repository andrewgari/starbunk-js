{
	"name": "bunkbot",
	"version": "0.0.1",
	"description": "it's bot that says blu, among things...",
	"scripts": {
		"build": "tsc",
		"start": "node dist/bunkbot.js",
		"dev": "npm run lint:fix && npm run build && npm run test",
		"test": "jest",
		"tests": "jest --config=jest.config.js",
		"test:coverage": "jest --coverage",
		"test:watch": "jest --watch",
		"lint": "eslint 'src/**/*.{ts,js}' '*.{ts,js}'",
		"lint:fix": "eslint 'src/**/*.{ts,js}' '*.{ts,js}' --fix",
		"format": "prettier --write \"src/**/*.ts\"",
		"type-check": "tsc --noEmit",
		"test:music": "jest src/tests/starbunk/commands/musicCommands.test.ts"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/andrewgari/starbunk-js.git"
	},
	"keywords": [
		"discord",
		"blue"
	],
	"author": "Andrew Gari",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/andrewgari/starbunk-js/issues"
	},
	"homepage": "https://github.com/andrewgari/starbunk-js#readme",
	"dependencies": {
		"@discordjs/builders": "^1.6.5",
		"@discordjs/rest": "^2.0.1",
		"@discordjs/voice": "^0.18.0",
		"@distube/yt-dlp": "^2.0.1",
		"@distube/ytdl-core": "^4.15.9",
		"async": "^3.2.4",
		"chalk": "^4.1.2",
		"discord-api-types": "^0.37.59",
		"discord.js": "^14.17.3",
		"dotenv": "^16.3.1",
		"envalid": "^8.0.0",
		"ffmpeg-static": "^5.2.0",
		"module-name": "^0.0.1-security",
		"openai": "^4.83.0",
		"opusscript": "^0.0.8",
		"play-dl": "^1.9.7",
		"ytdl-core": "^4.11.5"
	},
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@types/node": "^20.17.14",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"eslint": "^8.0.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.2.3",
		"jest": "^29.7.0",
		"prettier": "^3.2.5",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"tslib": "^2.6.2",
		"typescript": "^5.0.0"
	}
}
