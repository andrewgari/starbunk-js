{
  "name": "@starbunk/bluebot",
  "version": "1.28.0",
  "description": "BlueBot - Blue detection and response bot container",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc --build && tsc-alias",
    "build:docker": "tsc --build tsconfig.docker.json && tsc-alias -p tsconfig.docker.json",
    "start": "node dist/index.js",
    "dev": "cross-env NODE_ENV=development DEBUG=true ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "tester": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only --project tools/tsconfig.json tools/tester.ts",
    "tester:cli": "cross-env NODE_ENV=development ts-node --transpile-only -r tsconfig-paths/register --project tools/tsconfig.json tools/interactive-tester.ts",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:e2e": "vitest run tests/e2e",
    "type-check": "tsc --build --dry",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "check:all": "npm run type-check && npm run lint && npm run test",
    "clean": "tsc --build --clean",
    "deps:check": "node scripts/check-unused-deps.js",
    "deps:check-full": "npx --yes depcheck --ignores='@types/*,vitest,typescript,eslint,rimraf,cross-env'",
    "deps:list": "npm list --depth=0"
  },
  "dependencies": {
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "fastest-levenshtein": "^1.0.16",
    "loglayer": "^8.4.0",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "@types/node": "^25.0.8",
    "cross-env": "^10.1.0",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.9.3",
    "vitest": "^4.0.17"
  }
}
