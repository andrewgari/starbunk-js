{
	"name": "@starbunk/bunkbot",
	"version": "1.0.0",
	"description": "BunkBot - Reply bots and admin commands container",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc && tsc-alias",
		"start": "node dist/index.js",
		"dev": "cross-env NODE_ENV=development DEBUG=true ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
		"test": "jest",
		"type-check": "tsc --noEmit",
		"lint": "eslint src/**/*.ts",
		"lint:fix": "eslint src/**/*.ts --fix",
		"check:all": "npm run type-check && npm run lint && npm run test",
		"clean": "rimraf dist"
	},
	"dependencies": {
		"@discordjs/builders": "^1.6.5",
		"@discordjs/rest": "^2.5.0",
		"@prisma/client": "^5.22.0",
		"@starbunk/shared": "file:../shared",
		"async": "^3.2.4",
		"chalk": "^4.1.2",
		"discord-api-types": "^0.38.2",
		"discord.js": "^14.18.0",
		"dotenv": "^16.5.0",
		"rxjs": "^7.8.2",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@types/node": "^20.17.14",
		"cross-env": "^7.0.3",
		"jest": "^29.7.0",
		"rimraf": "^5.0.1",
		"ts-jest": "^29.2.6",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsc-alias": "^1.8.11",
		"tsconfig-paths": "^4.2.0",
		"typescript": "~5.3.0"
	}
}
