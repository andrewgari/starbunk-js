{
	"name": "@starbunk/covabot",
	"version": "1.0.0",
	"description": "CovaBot - AI personality bot container",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc && tsc-alias",
		"start": "node dist/index.js",
		"start:web": "node dist/index-web.js",
		"dev": "cross-env NODE_ENV=development DEBUG=true ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
		"dev:web": "cross-env NODE_ENV=development DEBUG=true ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index-web.ts",
		"test": "jest",
		"type-check": "tsc --noEmit",
		"lint": "eslint src/**/*.ts",
		"lint:fix": "eslint src/**/*.ts --fix",
		"check:all": "npm run type-check && npm run lint && npm run test",
		"clean": "rimraf dist",
		"migrate-to-qdrant": "ts-node -r tsconfig-paths/register src/scripts/migrate-to-qdrant.ts",
		"start:qdrant": "cross-env USE_QDRANT=true node dist/index-web.js",
		"dev:qdrant": "cross-env NODE_ENV=development DEBUG=true USE_QDRANT=true ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index-web.ts",
		"demo": "ts-node -r tsconfig-paths/register src/scripts/demo-personality-system.ts",
		"demo:qdrant": "cross-env USE_QDRANT=true ts-node -r tsconfig-paths/register src/scripts/demo-personality-system.ts",
		"test:persistence": "ts-node -r tsconfig-paths/register src/scripts/test-persistence.ts",
		"test:unraid": "ts-node -r tsconfig-paths/register src/scripts/test-unraid-deployment.ts",
		"test:unraid:simple": "ts-node -r tsconfig-paths/register src/scripts/test-unraid-simple.ts",
		"test:unraid:full": "npm run test:unraid && npm run test:persistence",
		"test:web": "node test-web-interface.js",
		"validate": "node validate-production.js"
	},
	"dependencies": {
		"@discordjs/builders": "^1.6.5",
		"@discordjs/rest": "^2.5.0",
		"@qdrant/js-client-rest": "^1.14.1",
		"@starbunk/shared": "file:../shared",
		"@xenova/transformers": "^2.17.2",
		"chalk": "^4.1.2",
		"cors": "^2.8.5",
		"discord.js": "^14.18.0",
		"dotenv": "^16.5.0",
		"express": "^5.1.0",
		"multer": "^2.0.1",
		"openai": "^4.95.1",
		"rxjs": "^7.8.2",
		"uuid": "^11.1.0",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.3",
		"@types/jest": "^29.5.14",
		"@types/node": "^20.17.14",
		"@types/supertest": "^6.0.2",
		"@types/uuid": "^10.0.0",
		"cross-env": "^7.0.3",
		"jest": "^29.7.0",
		"rimraf": "^5.0.1",
		"supertest": "^7.0.0",
		"ts-jest": "^29.2.6",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsc-alias": "^1.8.11",
		"tsconfig-paths": "^4.2.0",
		"typescript": "~5.3.0"
	}
}
