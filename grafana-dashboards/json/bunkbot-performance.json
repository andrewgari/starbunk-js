{
  "title": "Starbunk - BunkBot Performance",
  "uid": "starbunk-bunkbot-perf",
  "schemaVersion": 38,
  "version": 1,
  "editable": true,
  "timezone": "browser",
  "panels": [
    {
      "type": "stat",
      "title": "Triggers rate (5m)",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
      "targets": [
        { "expr": "sum(rate(bunkbot_bot_triggers_total[5m])) by (bot_name)", "legendFormat": "{{bot_name}}", "datasource": "Prometheus" }
      ]
    },
    {
      "type": "stat",
      "title": "Responses rate (5m)",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
      "targets": [
        { "expr": "sum(rate(bunkbot_bot_responses_total[5m])) by (bot_name, response_type)", "legendFormat": "{{bot_name}}/{{response_type}}", "datasource": "Prometheus" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Response p90 duration (ms)",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "targets": [
        { "expr": "histogram_quantile(0.90, sum by (le, bot_name) (rate(bunkbot_bot_response_duration_ms_bucket[5m])))", "legendFormat": "p90 {{bot_name}}", "datasource": "Prometheus" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Message processing p90 (ms)",
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 8 },
      "targets": [
        { "expr": "histogram_quantile(0.90, sum by (le, channel_type) (rate(bunkbot_message_processing_duration_ms_bucket[5m])))", "legendFormat": "p90 {{channel_type}}", "datasource": "Prometheus" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Concurrent message processing",
      "gridPos": { "x": 12, "y": 8, "w": 6, "h": 6 },
      "targets": [
        { "expr": "bunkbot_concurrent_message_processing", "legendFormat": "concurrency", "datasource": "Prometheus" }
      ]
    },
    {
      "type": "stat",
      "title": "Skips rate by reason (5m)",
      "gridPos": { "x": 18, "y": 8, "w": 6, "h": 6 },
      "targets": [
        { "expr": "sum(rate(bunkbot_bot_skips_total[5m])) by (skip_reason)", "legendFormat": "{{skip_reason}}", "datasource": "Prometheus" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Circuit breaker state by bot (0=closed,1=open,2=half)",
      "gridPos": { "x": 0, "y": 12, "w": 12, "h": 6 },
      "targets": [
        { "expr": "bunkbot_circuit_breaker_state", "legendFormat": "{{bot_name}}", "datasource": "Prometheus" }
      ]
    },
    {
      "type": "stat",
      "title": "CB transitions rate (5m)",
      "gridPos": { "x": 12, "y": 14, "w": 12, "h": 4 },
      "targets": [
        { "expr": "sum(rate(bunkbot_circuit_breaker_transitions_total[5m])) by (bot_name, to_state)", "legendFormat": "{{bot_name}} -> {{to_state}}", "datasource": "Prometheus" }
      ]
    },
    {
      "type": "stat",
      "title": "Webhook success rate (5m)",
      "gridPos": { "x": 0, "y": 18, "w": 6, "h": 4 },
      "targets": [
        { "expr": "sum(rate(bunkbot_webhook_deliveries_total{success=\"true\"}[5m])) / clamp_min(sum(rate(bunkbot_webhook_deliveries_total[5m])), 1)", "legendFormat": "success ratio", "datasource": "Prometheus" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "orientation": "horizontal" }
    },
    {
      "type": "timeseries",
      "title": "Webhook delivery p90 (ms)",
      "gridPos": { "x": 6, "y": 18, "w": 6, "h": 4 },
      "targets": [
        { "expr": "histogram_quantile(0.90, sum by (le, bot_name) (rate(bunkbot_webhook_delivery_duration_ms_bucket[5m])))", "legendFormat": "{{bot_name}}", "datasource": "Prometheus" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Registry load p90 (ms)",
      "gridPos": { "x": 12, "y": 18, "w": 6, "h": 4 },
      "targets": [
        { "expr": "histogram_quantile(0.90, sum by (le, load_type) (rate(bunkbot_registry_load_duration_ms_bucket[5m])))", "legendFormat": "{{load_type}}", "datasource": "Prometheus" }
      ]
    },
    {
      "type": "stat",
      "title": "Bots loaded",
      "gridPos": { "x": 18, "y": 18, "w": 6, "h": 4 },
      "targets": [
        { "expr": "sum(bunkbot_registry_bot_count)", "legendFormat": "bots", "datasource": "Prometheus" }
      ]
    },
    {
      "type": "logs",
      "title": "Logs (bunkbot)",
      "gridPos": { "x": 0, "y": 22, "w": 24, "h": 8 },
      "datasource": "Loki",
      "targets": [
        { "expr": "{com_starbunk_service=\"bunkbot\"}", "refId": "A" }
      ]
    }
  ]
}

