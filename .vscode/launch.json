{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Debug BunkBot",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}/containers/bunkbot/src/index.ts",
			"outFiles": ["${workspaceFolder}/containers/bunkbot/dist/**/*.js"],
			"env": {
				"LOG_LEVEL": "debug"
			}
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Debug DJCova",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}/containers/djcova/src/index.ts",
			"outFiles": ["${workspaceFolder}/containers/djcova/dist/**/*.js"],
			"env": {
				"LOG_LEVEL": "debug"
			}
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Debug Starbunk-DND",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}/containers/starbunk-dnd/src/index.ts",
			"outFiles": ["${workspaceFolder}/containers/starbunk-dnd/dist/**/*.js"],
			"env": {
				"LOG_LEVEL": "debug"
			}
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Debug CovaBot",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}/containers/covabot/src/index.ts",
			"outFiles": ["${workspaceFolder}/containers/covabot/dist/**/*.js"],
			"env": {
				"LOG_LEVEL": "debug"
			}
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Attach to Running Process",
			"port": 9229,
			"skipFiles": ["<node_internals>/**"]
		}
	]
}
