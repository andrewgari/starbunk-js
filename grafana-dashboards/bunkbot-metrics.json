{
  "dashboard": {
    "id": null,
    "uid": "starbunk-bunkbot",
    "title": "Starbunk Discord Bot - BunkBot Reply Bots",
    "tags": ["starbunk", "discord", "bunkbot", "reply-bots"],
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 37,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "name": "bot",
          "type": "query",
          "query": "label_values(bot_triggers_total{service=\"bunkbot\"}, bot)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        },
        {
          "name": "channel",
          "type": "query",
          "query": "label_values(bot_triggers_total{service=\"bunkbot\", bot=~\"$bot\"}, channel_name)",
          "refresh": 2,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Active Reply Bots",
        "type": "stat",
        "targets": [
          {
            "expr": "count by () (bot_instances_loaded{service=\"bunkbot\"})",
            "legendFormat": "Loaded Bots"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 5
                },
                {
                  "color": "green",
                  "value": 10
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Bot Triggers (Last Hour)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(bot_triggers_total{service=\"bunkbot\", bot=~\"$bot\", channel_name=~\"$channel\"}[1h]))",
            "legendFormat": "Triggers"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Successful Responses (Last Hour)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(bot_responses_total{service=\"bunkbot\", bot=~\"$bot\", channel_name=~\"$channel\"}[1h]))",
            "legendFormat": "Responses"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Response Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(bot_responses_total{service=\"bunkbot\", bot=~\"$bot\", channel_name=~\"$channel\"}[1h])) / sum(increase(bot_triggers_total{service=\"bunkbot\", bot=~\"$bot\", channel_name=~\"$channel\"}[1h])) * 100",
            "legendFormat": "Success Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 80
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Bot Trigger Rate by Bot",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum by (bot) (rate(bot_triggers_total{service=\"bunkbot\", bot=~\"$bot\", channel_name=~\"$channel\"}[5m]))",
            "legendFormat": "{{bot}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "axisGridShow": true,
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 6
        }
      },
      {
        "id": 6,
        "title": "Bot Response Latency",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(bot_response_duration_ms_bucket{service=\"bunkbot\", bot=~\"$bot\", channel_name=~\"$channel\"}[5m])) by (le, bot))",
            "legendFormat": "{{bot}} - 50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(bot_response_duration_ms_bucket{service=\"bunkbot\", bot=~\"$bot\", channel_name=~\"$channel\"}[5m])) by (le, bot))",
            "legendFormat": "{{bot}} - 95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(bot_response_duration_ms_bucket{service=\"bunkbot\", bot=~\"$bot\", channel_name=~\"$channel\"}[5m])) by (le, bot))",
            "legendFormat": "{{bot}} - 99th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "axisGridShow": true,
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 500
                },
                {
                  "color": "red",
                  "value": 1000
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 6
        }
      },
      {
        "id": 7,
        "title": "Bot Skip Reasons",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (reason) (increase(bot_skips_total{service=\"bunkbot\", bot=~\"$bot\", channel_name=~\"$channel\"}[1h]))",
            "legendFormat": "{{reason}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "displayMode": "table",
              "calculation": "lastNotNull"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 14
        }
      },
      {
        "id": 8,
        "title": "Circuit Breaker Activity",
        "type": "timeseries",
        "targets": [
          {
            "expr": "increase(circuit_breaker_activations_total{service=\"bunkbot\", bot=~\"$bot\"}[5m])",
            "legendFormat": "{{bot}} - {{reason}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "bars",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 100,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "axisGridShow": true,
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 14
        }
      },
      {
        "id": 9,
        "title": "Top Performing Bots",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum by (bot) (increase(bot_triggers_total{service=\"bunkbot\", bot=~\"$bot\", channel_name=~\"$channel\"}[1h])))",
            "legendFormat": "",
            "format": "table",
            "instant": true,
            "refId": "A"
          },
          {
            "expr": "topk(10, sum by (bot) (increase(bot_responses_total{service=\"bunkbot\", bot=~\"$bot\", channel_name=~\"$channel\"}[1h])))",
            "legendFormat": "",
            "format": "table",
            "instant": true,
            "refId": "B"
          }
        ],
        "transformations": [
          {
            "id": "seriesToColumns",
            "options": {
              "byField": "bot"
            }
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "Time": true,
                "job": true,
                "instance": true,
                "service": true
              },
              "indexByName": {
                "bot": 0,
                "Value #A": 1,
                "Value #B": 2
              },
              "renameByName": {
                "bot": "Bot Name",
                "Value #A": "Triggers",
                "Value #B": "Responses"
              }
            }
          },
          {
            "id": "calculateField",
            "options": {
              "alias": "Success Rate %",
              "binary": {
                "left": "Responses",
                "operator": "/",
                "right": "Triggers"
              },
              "mode": "binary",
              "reduce": {
                "reducer": "sum"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto",
              "inspect": false
            },
            "mappings": []
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Triggers"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "short"
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Responses"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "short"
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Success Rate %"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "percentunit"
                }
              ]
            }
          ]
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 22
        }
      },
      {
        "id": 10,
        "title": "Bot Activity by Channel",
        "type": "heatmap",
        "targets": [
          {
            "expr": "sum by (channel_name, bot) (increase(bot_triggers_total{service=\"bunkbot\", bot=~\"$bot\", channel_name=~\"$channel\"}[5m]))",
            "legendFormat": "{{channel_name}} - {{bot}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "scaleDistribution": {
                "type": "linear"
              }
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 30
        }
      }
    ]
  }
}