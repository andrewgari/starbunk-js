{
	"name": "@starbunk/starbunk-dnd",
	"version": "1.0.0",
	"description": "Starbunk-DND - D&D features and Snowbunk bridge container",
	"main": "dist/index-minimal.js",
	"scripts": {
		"build": "tsc && tsc-alias",
		"build:docker": "tsc -p tsconfig.docker.json && tsc-alias -p tsconfig.docker.json",
		"start": "node dist/index-minimal.js",
		"dev": "cross-env NODE_ENV=development DEBUG=true ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index-minimal.ts",
		"test": "jest",
		"type-check": "tsc --noEmit",
		"clean": "rimraf dist"
	},
	"dependencies": {
		"@starbunk/shared": "file:../../packages/shared",
		"@discordjs/builders": "^1.6.5",
		"@discordjs/rest": "^2.5.0",
		"@xenova/transformers": "^2.17.2",
		"discord.js": "^14.18.0",
		"@prisma/client": "^6.7.0",
		"dotenv": "^16.5.0",
		"mammoth": "^1.9.0",
		"node-schedule": "^2.1.1",
		"uuid": "^11.1.0",
		"date-fns": "^4.1.0",
		"chalk": "^4.1.2",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@types/node": "^20.17.14",
		"@types/node-schedule": "^2.1.7",
		"@types/jest": "^29.5.14",
		"typescript": "~5.3.0",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsc-alias": "^1.8.11",
		"tsconfig-paths": "^4.2.0",
		"jest": "^29.7.0",
		"ts-jest": "^29.2.6",
		"cross-env": "^7.0.3",
		"rimraf": "^5.0.1"
	}
}
