		"@starbunk/shared": "file:../shared",
		"@xenova/transformers": "^2.17.2",
		"chalk": "^4.1.2",
		"discord.js": "^14.18.0",
		"dotenv": "^16.5.0",
		"openai": "^4.95.1",
		"rxjs": "^7.8.2",
		"uuid": "^11.1.0",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@types/node": "^20.17.14",
		"@types/uuid": "^10.0.0",
		"cross-env": "^7.0.3",
		"jest": "^29.7.0",
		"rimraf": "^5.0.1",
		"ts-jest": "^29.2.6",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsc-alias": "^1.8.11",
