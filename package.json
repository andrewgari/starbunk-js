{
  "name": "bunkbot",
  "version": "1.30.1",
  "description": "it's bot that says blu, among things...",
  "type": "commonjs",
  "workspaces": [
    "src/*"
  ],
  "scripts": {
    "sync-versions": "bash ./scripts/sync-versions.sh",
    "build": "npm run build:shared && npm run build:containers",
    "build:clean": "tsc --build --clean && npm run build",
    "build:shared": "tsc --build src/shared && npx tsc-alias -p src/shared/tsconfig.json --debug",
    "build:containers": "npm run build:bunkbot && npm run build:djcova && npm run build:covabot && npm run build:bluebot",
    "build:bunkbot": "tsc --build src/bunkbot && npx tsc-alias -p src/bunkbot/tsconfig.json --verbose",
    "build:djcova": "tsc --build src/djcova && npx tsc-alias -p src/djcova/tsconfig.json --verbose",
    "build:covabot": "tsc --build src/covabot && npx tsc-alias -p src/covabot/tsconfig.json --verbose",
    "build:bluebot": "tsc --build --clean src/bluebot && tsc --build src/bluebot && cd src/bluebot && npx tsc-alias && cd ../..",
    "start": "docker compose up -d",
    "test": "npm run test:shared && npm run test:containers",
    "test:full": "npm run test:shared && npm run test:containers",
    "test:shared": "cd src/shared && npm test",
    "test:containers": "npm run test:bunkbot && npm run test:djcova && npm run test:covabot && npm run test:bluebot",
    "test:bunkbot": "cd src/bunkbot && npm test",
    "test:djcova": "cd src/djcova && npm test",
    "test:covabot": "cd src/covabot && npm test",
    "test:bluebot": "cd src/bluebot && npm test",
    "lint": "eslint src/*/src/**/*.ts",
    "lint:fix": "eslint src/*/src/**/*.ts --fix",
    "type-check": "tsc --build --dry",
    "clean": "tsc --build --clean",
    "docker:build": "docker compose -f infrastructure/docker/docker-compose.yml build",
    "check:all": "npm run type-check && npm run lint && npm run test",
    "check:ci": "npm run type-check && npm run lint && npm run test",
    "health-check": "scripts/monitoring/structure-health-check.sh",
    "ralph:pr-comments": "bash scripts/ai/ralph-pr-comments.sh",
    "ralph:fix": "bash scripts/ai/ralph-fix-errors.sh",
    "setup:config": "bash scripts/setup-config.sh",
    "release": "semantic-release"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewgari/starbunk-js.git"
  },
  "keywords": [
    "discord",
    "blue"
  ],
  "author": "Andrew Gari",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andrewgari/starbunk-js/issues"
  },
  "homepage": "https://github.com/andrewgari/starbunk-js#readme",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@loglayer/transport-pino": "^2.2.17",
    "@types/ioredis": "^4.28.10",
    "axios": "^1.11.0",
    "dotenv": "^16.6.1",
    "esbuild": "^0.25.11",
    "ioredis": "^5.8.0",
    "loglayer": "^8.4.0",
    "pino": "^10.2.0",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.6.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^8.56.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.1",
    "semantic-release": "^25.0.2",
    "tsc-alias": "^1.8.16",
    "typescript": "~5.3.0",
    "zod-to-json-schema": "^3.25.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache",
      "prettier --write"
    ]
  },
  "overrides": {
    "discord.js": "^14.18.0",
    "inflight": "npm:@aashutoshrathi/inflight@^1.0.0",
    "glob": "^10.0.0",
    "undici": ">=6.23.0",
    "tar": ">=7.5.3",
    "diff": ">=8.0.3"
  }
}
