{
  "dashboard": {
    "id": null,
    "uid": "starbunk-covabot",
    "title": "Starbunk Discord Bot - CovaBot AI Personality",
    "tags": ["starbunk", "discord", "covabot", "ai", "personality", "llm"],
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 37,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "name": "guild_id",
          "type": "query",
          "query": "label_values(discord_messages_processed_total{service=\"covabot\"}, guild_id)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        },
        {
          "name": "user_id",
          "type": "query",
          "query": "label_values(covabot_conversations_total{service=\"covabot\", guild_id=~\"$guild_id\"}, user_id)",
          "refresh": 2,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "AI Conversations (Last Hour)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(covabot_conversations_total{service=\"covabot\", guild_id=~\"$guild_id\", user_id=~\"$user_id\"}[1h]))",
            "legendFormat": "Conversations"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Personality Responses (Last Hour)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(covabot_personality_responses_total{service=\"covabot\", guild_id=~\"$guild_id\"}[1h]))",
            "legendFormat": "Responses"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Memory Operations (Last Hour)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(covabot_memory_operations_total{service=\"covabot\", guild_id=~\"$guild_id\"}[1h]))",
            "legendFormat": "Memory Ops"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Average Response Sentiment",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(covabot_response_sentiment{service=\"covabot\", guild_id=~\"$guild_id\"})",
            "legendFormat": "Sentiment Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": -1,
            "max": 1,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": -1
                },
                {
                  "color": "yellow",
                  "value": -0.2
                },
                {
                  "color": "green",
                  "value": 0.2
                },
                {
                  "color": "blue",
                  "value": 0.6
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "AI Conversation Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum by (guild_id) (rate(covabot_conversations_total{service=\"covabot\", guild_id=~\"$guild_id\", user_id=~\"$user_id\"}[5m]))",
            "legendFormat": "Guild {{guild_id}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "axisGridShow": true,
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 6
        }
      },
      {
        "id": 6,
        "title": "LLM Response Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(covabot_llm_response_duration_ms_bucket{service=\"covabot\", guild_id=~\"$guild_id\"}[5m])) by (le, llm_provider))",
            "legendFormat": "{{llm_provider}} - 50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(covabot_llm_response_duration_ms_bucket{service=\"covabot\", guild_id=~\"$guild_id\"}[5m])) by (le, llm_provider))",
            "legendFormat": "{{llm_provider}} - 95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(covabot_llm_response_duration_ms_bucket{service=\"covabot\", guild_id=~\"$guild_id\"}[5m])) by (le, llm_provider))",
            "legendFormat": "{{llm_provider}} - 99th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "axisGridShow": true,
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 3000
                },
                {
                  "color": "red",
                  "value": 8000
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 6
        }
      },
      {
        "id": 7,
        "title": "Memory System Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum by (operation) (rate(covabot_memory_operations_total{service=\"covabot\", guild_id=~\"$guild_id\"}[5m]))",
            "legendFormat": "{{operation}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "normal",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "axisGridShow": true,
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 14
        }
      },
      {
        "id": 8,
        "title": "Personality Trait Distribution",
        "type": "timeseries",
        "targets": [
          {
            "expr": "covabot_personality_trait_strength{service=\"covabot\", guild_id=~\"$guild_id\"}",
            "legendFormat": "{{trait_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "max": 1,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 20,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "axisGridShow": true,
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 14
        }
      },
      {
        "id": 9,
        "title": "Most Active Users",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum by (user_name, user_id) (increase(covabot_conversations_total{service=\"covabot\", guild_id=~\"$guild_id\", user_id=~\"$user_id\"}[1h])))",
            "legendFormat": "",
            "format": "table",
            "instant": true,
            "refId": "A"
          },
          {
            "expr": "topk(10, avg by (user_name, user_id) (covabot_user_sentiment{service=\"covabot\", guild_id=~\"$guild_id\", user_id=~\"$user_id\"})) ",
            "legendFormat": "",
            "format": "table",
            "instant": true,
            "refId": "B"
          }
        ],
        "transformations": [
          {
            "id": "seriesToColumns",
            "options": {
              "byField": "user_name"
            }
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "Time": true,
                "job": true,
                "instance": true,
                "service": true,
                "guild_id": true
              },
              "indexByName": {
                "user_name": 0,
                "user_id": 1,
                "Value #A": 2,
                "Value #B": 3
              },
              "renameByName": {
                "user_name": "User",
                "user_id": "User ID",
                "Value #A": "Conversations (1h)",
                "Value #B": "Avg Sentiment"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto",
              "inspect": false
            },
            "mappings": []
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Conversations (1h)"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "short"
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Avg Sentiment"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "short"
                },
                {
                  "id": "color",
                  "value": {
                    "mode": "thresholds"
                  }
                },
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {
                        "color": "red",
                        "value": -1
                      },
                      {
                        "color": "yellow",
                        "value": 0
                      },
                      {
                        "color": "green",
                        "value": 0.3
                      }
                    ]
                  }
                }
              ]
            }
          ]
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 22
        }
      },
      {
        "id": 10,
        "title": "Response Context Sources",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (context_source) (increase(covabot_context_retrievals_total{service=\"covabot\", guild_id=~\"$guild_id\"}[1h]))",
            "legendFormat": "{{context_source}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "displayMode": "table",
              "calculation": "lastNotNull"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 22
        }
      },
      {
        "id": 11,
        "title": "Conversation Length Distribution",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(covabot_conversation_length_bucket{service=\"covabot\", guild_id=~\"$guild_id\"}[5m])) by (le))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.75, sum(rate(covabot_conversation_length_bucket{service=\"covabot\", guild_id=~\"$guild_id\"}[5m])) by (le))",
            "legendFormat": "75th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(covabot_conversation_length_bucket{service=\"covabot\", guild_id=~\"$guild_id\"}[5m])) by (le))",
            "legendFormat": "95th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "axisGridShow": true,
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 30
        }
      },
      {
        "id": 12,
        "title": "AI Model Usage & Errors",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum by (llm_provider) (rate(covabot_llm_queries_total{service=\"covabot\", guild_id=~\"$guild_id\"}[5m]))",
            "legendFormat": "{{llm_provider}} - Queries"
          },
          {
            "expr": "sum by (llm_provider, error_type) (rate(covabot_llm_errors_total{service=\"covabot\", guild_id=~\"$guild_id\"}[5m])) * 10",
            "legendFormat": "{{llm_provider}} - {{error_type}} × 10"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "axisGridShow": true,
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byRegexp",
                "options": ".*error.*"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "fixedColor": "red",
                    "mode": "fixed"
                  }
                }
              ]
            }
          ]
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 30
        }
      }
    ]
  }
}