{
	"name": "bunkbot",
	"version": "0.0.2",
	"description": "it's bot that says blu, among things...",
	"scripts": {
		"build": "tsc",
		"start": "node dist/bunkbot.js",
		"dev": "ts-node src/bunkbot.ts",
		"test": "jest",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix",
		"format": "prettier --write \"src/**/*.ts\"",
		"type-check": "tsc --noEmit",
		"type-check:relaxed": "tsc --project tsconfig-check.json --noEmit",
		"clean": "rimraf dist",
		"build:clean": "npm run clean && npm run build",
		"check:docker": "docker build -t starbunk-js:latest .",
		"check:docker:boot": "node scripts/check-docker-boot.js",
		"check:types": "npm run type-check:relaxed",
		"check:lint": "npm run lint",
		"check:build": "npm run build",
		"check:all": "npm run check:types && npm run check:lint && npm run test && npm run check:build && npm run check:docker && npm run check:docker:boot"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/andrewgari/starbunk-js.git"
	},
	"keywords": [
		"discord",
		"blue"
	],
	"author": "Andrew Gari",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/andrewgari/starbunk-js/issues"
	},
	"homepage": "https://github.com/andrewgari/starbunk-js#readme",
	"dependencies": {
		"@discordjs/builders": "^1.6.5",
		"@discordjs/rest": "^2.0.1",
		"@discordjs/voice": "^0.18.0",
		"@distube/yt-dlp": "^2.0.1",
		"@distube/ytdl-core": "^4.15.9",
		"async": "^3.2.4",
		"chalk": "^4.1.2",
		"delta": "^3.0.5",
		"discord-api-types": "^0.37.59",
		"discord.js": "^14.18.0",
		"dotenv": "^16.3.1",
		"envalid": "^8.0.0",
		"ffmpeg-static": "^5.2.0",
		"module-alias": "^2.2.3",
		"module-name": "^0.0.1-security",
		"openai": "^4.58.2",
		"opusscript": "^0.0.8",
		"play-dl": "^1.9.7",
		"ytdl-core": "^4.11.5"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^6.6.3",
		"@types/jest": "^29.5.14",
		"@types/node": "^20.17.14",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"eslint": "^8.0.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.2.3",
		"jest": "^29.7.0",
		"prettier": "^3.2.5",
		"rimraf": "^5.0.1",
		"source-map-support": "^0.5.21",
		"ts-jest": "^29.2.6",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"tslib": "^2.6.2",
		"typescript": "^5.8.2"
	}
}
